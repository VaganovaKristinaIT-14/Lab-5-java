public class Cat implements Meowable {

  private final String name;

  public String getName() {
    return this.name;
  }


  public Cat(String name) {
    this.name = name;
  }

  @Override
  public String toString() {
    return "Кот: " + this.name;
  }

  @Override
  public void meow() {
    System.out.println(this.name + ": мяу!");
  }

}
